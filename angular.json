{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version":1, 
    "projects": {
        "mainapp":{
            "projectType": "application",
            "schematics": {

            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "ashik"
           
        },
        "TestApp": {
            "projectType": "application",
            "schematics": {},
            "root": "TestApp",
            "sourceRoot": "TestApp/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/TestApp",
                        "index": "TestApp/src/index.html",
                        "main": "TestApp/src/main.ts",
                        "polyfills": "TestApp/src/polyfills.ts",
                        "tsConfig": "TestApp/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "TestApp/src/favicon.ico",
                            "TestApp/src/assets"
                        ],
                        "styles": [
                            "TestApp/src/styles.css"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "TestApp/src/environments/environment.ts",
                                    "with": "TestApp/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "TestApp:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "TestApp:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "TestApp:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "TestApp/src/test.ts",
                        "polyfills": "TestApp/src/polyfills.ts",
                        "tsConfig": "TestApp/tsconfig.spec.json",
                        "karmaConfig": "TestApp/karma.conf.js",
                        "assets": [
                            "TestApp/src/favicon.ico",
                            "TestApp/src/assets"
                        ],
                        "styles": [
                            "TestApp/src/styles.css"
                        ],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "TestApp/tsconfig.app.json",
                            "TestApp/tsconfig.spec.json",
                            "TestApp/e2e/tsconfig.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "TestApp/e2e/protractor.conf.js",
                        "devServerTarget": "TestApp:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "TestApp:serve:production"
                        }
                    }
                }
            }
        }
    }

}